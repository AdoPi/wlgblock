+	call .compare_strings
+	jr nz, .nicknames_mismatch
+
+	ld hl, wPartyMon5Nickname
+       	ld de, NicknamesPass5
+	call .compare_strings
+	jr nz, .nicknames_mismatch
+
+	ld hl, wPartyMon6Nickname
+       	ld de, NicknamesPass6
+	call .compare_strings
+	jr nz, .nicknames_mismatch
+
+.all_match_check:
+	ld a, 99
+	ld [$DCC7], a
+	ret
+
+; Compare hl with de
+.compare_strings:
+    ld a, [hl] 
+    ld b, a 
+    ld a, [de] 
+    cp b
+    jr nz, .strings_not_equal 
+
+    cp "@" 
+    jr z, .strings_equal 
+
+    inc hl 
+    inc de 
+    jr .compare_strings 
+
+.strings_not_equal:
+    jp .nicknames_mismatch
+    ret
+
+.strings_equal: 
+    ld a, 1 
+    ret
+
+.nicknames_mismatch:
+	ld a, 02
+	ld [$C007], a
+	ret
