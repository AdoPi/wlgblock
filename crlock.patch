+NicknamesPass2:    db "is@", 0
+NicknamesPass3:    db "My@", 0
+NicknamesPass4:    db "Pass@", 0
+NicknamesPass5:    db "Word@", 0
+NicknamesPass6:    db "!!!@", 0
+
+
+DumbCheckPartyPassword:
+	; init
+	ld a, 01
+	ld [$C007], a
+
+	ld hl, wPartyMon1Nickname
+       	ld de, NicknamesPass1
+	call .compare_strings
+	jr nz, .nicknames_mismatch
+
+	ld hl, wPartyMon2Nickname
+       	ld de, NicknamesPass2
+	call .compare_strings
+	jr nz, .nicknames_mismatch
+
+	ld hl, wPartyMon3Nickname
+       	ld de, NicknamesPass3
+	call .compare_strings
+	jr nz, .nicknames_mismatch
+
+	ld hl, wPartyMon4Nickname
